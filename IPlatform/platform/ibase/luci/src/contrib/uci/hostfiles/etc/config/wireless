config mac-filter 'filter'
	option enable 'on'
	option action 'deny'

config wps-device 'wps'
	option wps_url	'http://192.168.1.1/'
	option wps_manufacturer	'www.tp-link.com'
	option wps_device_type	'6-0050F204-1'
	option wps_device_name	'AC1900'
	option wps_os_version	'1.0'
	option wps_config	'0x0086'
	option wps_state	'configured'
