config wifi-device  'wifi0'
	option type	'qcawifi'
	option phy	'wifi0'
	option band	'5g'
	option macaddr	'00:03:7f:48:d9:c3'
	option country	'US'
	option hwmode	'ac'
	option htmode 'auto'
	option channel	'auto'
	option txpower	'high'
	option beacon_int '100'
	option rts '2346'
	option frag '2346'
	option wpa_group_rekey '0'
	option dtim_period '1'
	option wmm 'on'
	option shortgi 'on'
	option isolate 'on'

config wifi-iface 'ath0'
	option ifname	'ath0'
	option device	'wifi0'
	option network	'lan'
	option bridge	'br-lan'
	option mode	'ap'
	option wds 'on'
	option wps 'on'
	option wps_pbc	'on'
	option wps_state	'2'
	option wps_label 'on'
	option wps_pin '20530037'
	option wps_timeout '120'
	option enable 'on'
	option hidden 'off'
	option ssid	'TP-LINK_HOME_5G'
	option encryption 'none'
	option wep_mode 'auto'
	option wep_select '1'
	option wep_format1 'hex'
	option wep_type1 '64'
	option wep_key1 '1234567890'
	option wep_format2 'hex'
	option wep_type2 '64'
	option wep_key2 '1234567890'
	option wep_format3 'hex'
	option wep_type3 '64'
	option wep_key3 '1234567890'
	option wep_format4 'hex'
	option wep_type4 '64'
	option wep_key4 '1234567890'
	option psk_key 'abcdefgh'
	option psk_version 'auto'
	option psk_cipher 'auto'
	option server '192.168.1.100'
	option port '1812'
	option wpa_key 'abcdefgh'
	option wpa_version 'auto'
	option wpa_cipher 'auto'

config wifi-iface 'ath01'
	option ifname	'ath01'
	option device	'wifi0'
	option network	'lan'
	option bridge	'br-lan'
	option mode	'ap'
	option wds 'on'
	option guest 'on'
	option enable 'off'
	option hidden 'off'
	option ssid	'TP-LINK_GUEST_5G'
	option encryption 'none'
	option psk_key 'abcdefgh'
	option psk_version 'auto'
	option psk_cipher 'auto'
	option isolate 'off'
	option access 'on'

config wifi-iface 'ath02'
	option ifname	'ath02'
	option device	'wifi0'
	option network	'lan'
	option bridge	'br-lan'
	option mode	'sta'
	option wds 'on'
	option enable 'off'
	option ssid	'TP-LINK_BRIDGE_5G'
	option bssid	'any'
	option encryption 'none'
	option psk_key 'abcdefgh'
	option psk_version 'auto'
	option psk_cipher 'auto'
	option wep_mode 'auto'
	option wep_select '1'
	option wep_format1 'hex'
	option wep_type1 '64'
	option wep_key1 '1234567890'
	option wep_format2 'hex'
	option wep_type2 '64'
	option wep_key2 '1234567890'
	option wep_format3 'hex'
	option wep_type3 '64'
