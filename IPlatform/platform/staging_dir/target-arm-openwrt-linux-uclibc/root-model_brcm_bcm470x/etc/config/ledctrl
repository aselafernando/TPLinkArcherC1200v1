config rule 'LAN'
    option ledname 'LAN'
    option delayon '500'
    option delayoff '500'
    option duration '60000'

config rule 'USB1'
    option ledname 'USB1'
    option ledon '0'

config rule 'USB3'
    option ledname 'USB3'
    option ledon '0'
    
config rule 'WANO'
    option ledname 'WAN1'
    option delayon '500'
    option delayoff '500'
    option duration '60000'
    
config rule 'WANB'
    option ledname 'WANB'
    option delayon '500'
    option delayoff '500'
    option duration '60000'
    
config rule 'STATUS'
    option ledname 'STATUS'
    option delayon '500'
    option delayoff '500'
    option duration '60000'
    
config rule 'WPS_SUCCESS'
    option ledname 'WPS'
    option delayon '300000'
    option delayoff '0'
    option duration '300000'
    
config rule 'WPS_ERROR'
    option ledname 'WPS'
    option delayon '250'
    option delayoff '250'
    option duration '60000'
    
config rule 'WPS_OVERLAP'
    option ledname 'WPS'
    option delayon   '250'
    option delayoff  '250'
    option delaylast '2000'
    option interval  '500'
    option duration  '30000'
    
config rule 'WPS_INPROGRESS'
    option ledname 'WPS'
    option delayon '2000'
    option delayoff '1000'
    option duration '120000'
