config usbshare 'global'
	option svrname        'Akronite-Share'
	option samba          'on'
	option ftp            'on'
	option ftpex          'off'
	option ftpex_port     '21'
	option dlna           'on'
	option printer        'on'
	option share_all      'on'
	option auth_all       'off'

config usbshare 'account'
	option use_login_user 'on'
	option username       'admin'
	option password       'admin'

config volumn
	option serial 'usb_serial_number1'
	option uuid 'partition_uuid1'
	option label 'label or null'
	option capacity '808902656'
	option used '4194304'
	option enable 'on'

config volumn
	option serial 'usb_serial_number1'
	option uuid 'partition_uuid2'
	option label 'label or null'
	option capacity '808902656'
	option used '4194304'
	option enable 'on'

config folder 'alias'
	option index 'index1'
	option uuid 'partition_uuid1'
	option sharename 'sharename'
	option path 'path'
	option read_users 'admin,guest'
	option write_users 'admin,guest'
	option sharetype 'ftp,samba,dlna'
	option gnetwork 'on'
	option enable 'on'

config folder 'Movies'
	option index 'index2'
	option uuid 'partition_uuid1'
	option sharename 'Movies'
	option path 'media/movies'
	option read_users 'admin,guest'
	option write_users 'admin,guest'
	option sharetype 'ftp,samba,dlna'
	option gnetwork 'on'
	option enable 'on'
