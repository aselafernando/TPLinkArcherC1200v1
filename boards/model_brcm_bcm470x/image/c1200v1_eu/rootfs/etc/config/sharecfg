config device 
	option serial 'usb_serial_number1'
	option model 'model_name'
	option vendor 'vendor_name'
	option capacity '16043212800'

config volumn
	option serial 'usb_serial_number1'
	option uuid 'partition_uuid1'
	option label 'label or null'
	option devname '/dev/sda1'
	option mntdir '/mnt/sda1'
	option capacity '808902656'
	option used '4194304'
	option enable 'on'

config volumn
	option serial 'usb_serial_number1'
	option uuid 'partition_uuid2'
	option label 'label or null'
	option devname '/dev/sda2'
	option mntdir '/mnt/sda2'
	option capacity '808902656'
	option used '4194304'
	option enable 'on'

config folder 'alias'
	option index 'index1'
	option uuid 'partition_uuid1'
	option sharename 'sharename'
	option path 'path'
	option gnetwork 'on'
	option read_users 'admin,guest'
	option write_users 'admin,guest'
	option sharetype 'ftp,samba,dlna'
	option enable 'on'

config folder 'Movies'
	option index 'index2'
	option uuid 'partition_uuid1'
	option sharename 'Movies'
	option path 'media/movies'
	option gnetwork 'on'
	option read_users 'admin,guest'
	option write_users 'admin,guest'
	option sharetype 'ftp,samba,dlna'
	option enable 'on'
