config switch
    option name 'switch0'
    option enable_vlan '1'
    option reset '1'

config switch_vlan
    option ports '6t 1'
    option device 'switch0'
    option vlan '1'

config switch_vlan
    option ports '6t 2'
    option device 'switch0'
    option vlan '2'

config switch_vlan
    option ports '6t 3'
    option device 'switch0'
    option vlan '3'

config switch_vlan
    option ports '6t 4'
    option device 'switch0'
    option vlan '4'

config switch_vlan
    option ports '0 5'
    option device 'switch0'
    option vlan '5'